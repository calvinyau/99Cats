<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  <h2>Logged in: <%= current_user.user_name %></h2>
  <%= button_to "Logout", session_url(current_user), method: :delete %>
<% else %>
  <%= button_to "Login", new_session_url, method: :get %>
<% end %>

<%= yield %>

</body>
</html>
